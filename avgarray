Scanner s=new Scanner(System.in);
    int n,i,sum=0,avg;
    System.out.println("Enter the range");
     n=s.nextInt();
    int [] array=new int[n];
    System.out.println("Enter the Number");
    for(i=0;i<n;i++)
    {
    array[i]=s.nextInt();
    }
    for(i=0;i<n;i++)
    {
        sum+=array[i];
    }
    avg=sum/n;
    System.out.println("Average of array");
    System.out.println(avg);
